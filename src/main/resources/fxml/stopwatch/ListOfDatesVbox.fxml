<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXTableView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import io.github.palexdev.materialfx.controls.MFXButton?>
<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml"
         stylesheets="@../../css/common.css"
         styleClass="grid-background" style="-fx-padding: 10 10 10 10;" spacing="10" maxHeight="600">

    <Label maxWidth="1.7976931348623157E308" alignment="CENTER" styleClass="header-label">Dates</Label>

    <MFXTableView fx:id="table" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0"
                  prefWidth="275.0" footerVisible="false"/>

    <MFXButton buttonType="RAISED" text="Add" onAction="#add" maxWidth="1.7976931348623157E308" alignment="CENTER"/>
    <MFXButton buttonType="RAISED" text="Refresh" onAction="#refresh" maxWidth="1.7976931348623157E308" alignment="CENTER"/>
</fx:root>