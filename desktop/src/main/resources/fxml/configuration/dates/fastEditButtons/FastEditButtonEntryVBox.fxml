<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.HBox?>
<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml"
         stylesheets="@../../../../css/common.css"
         styleClass="grid-background" style="-fx-padding: 10 10 10 10;" spacing="10">

    <HBox spacing="3">
        <Text>Name:</Text>
        <Text fx:id="fastButtonNameText"/>
    </HBox>
    <HBox spacing="3">
        <Text>Expected time:</Text>
        <Text fx:id="fastButtonTimeText"/>
    </HBox>
    <Button onMouseClicked="#delete">Delete</Button>
</fx:root>