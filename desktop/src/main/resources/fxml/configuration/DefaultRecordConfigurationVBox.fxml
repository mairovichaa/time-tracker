         stylesheets="@../../css/common.css"
         style="-fx-padding: 10 10 10 10;" spacing="10">

    <Button maxWidth="1.7976931348623157E308" alignment="CENTER" styleClass="header-label"
            fx:id="addDefaultRecordNameButton" onMouseClicked="#add">Add default record name
