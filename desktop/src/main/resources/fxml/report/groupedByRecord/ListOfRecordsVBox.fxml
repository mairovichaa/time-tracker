<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXScrollPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml"
         stylesheets="@../../../css/common.css" minWidth="300" maxWidth="300" spacing="10">
    <MFXScrollPane fx:id="scrollPane" hbarPolicy="NEVER" styleClass="white-scroll-pane">
        <VBox fx:id="recordsVBox" spacing="15" style="-fx-background-color: white;" minWidth="295" maxWidth="295">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="6"/>
            </padding>
        </VBox>
    </MFXScrollPane>
</fx:root>